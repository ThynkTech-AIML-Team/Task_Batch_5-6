   "source": [
    "import cv2\n",
    "\n",
    "image = cv2.imread(\"cube.jpeg\")\n",
    "gray = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)\n",
    "\n",
    "edges = cv2.Canny(gray, 100, 200)\n",
    "resized = cv2.resize(edges, (600, 400))\n",
    "cv2.imshow(\"Edges\", resized)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
